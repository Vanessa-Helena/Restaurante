<body>
    <h1>Criar nova people</h1>
    <form action="/peoples" method="POST">
        <section>
            <label>Nome da people</label>
            <input type="text" id="nome" placeholder="Nome da people" name="nome"></input>
        </section>
        <section>
            <label>Registro da people</label>
            <input type="number" id="registro" placeholder="Registro da people" name="registro"></input>
        </section>
        <button type="submit">Enviar</button>
    </form>
        <a href="/peoples">Voltar</a>
</body>
